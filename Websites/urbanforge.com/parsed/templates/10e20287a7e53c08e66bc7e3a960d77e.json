{"components/common/navigation-right":"<nav class=\"navUser navUser--right\">\r\n    <ul class=\"navUser-section navUser-section--alt\">\r\n        {{#if theme_settings.social_icon_placement_top}}\r\n            <li class=\"navUser-item navUser-item--social\">\r\n                {{> components/common/social-links}}\r\n            </li>\r\n        {{/if}}\r\n        <li class=\"navUser-item navUser-item--contact\">\r\n            <a class=\"navUser-action navUser-action--contact\" href=\"tel:1-800-380-4766\">1-800-380-4766</a>\r\n        </li>\r\n        <li class=\"navUser-item navUser-item--location\">\r\n            <a class=\"navUser-action navUser-action--location\" href=\"/store-location/\"><span class=\"navUser-item-icon navUser-item-locationIcon emthemesModez-icon-location-pin\"></span>\r\n                <span class=\"navUser-action-locationLabel\">Locations</span>\r\n        \r\n            </a>\r\n        </li>\r\n        <li class=\"navUser-item navUser-item--quickSearch\">\r\n            <a class=\"navUser-action navUser-action--quickSearch snize-custom-widget-opener\" href=\"#\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\">\r\n                <span class=\"navUser-item-icon navUser-item-searchIcon emthemesModez-icon-linear-magnifier\"></span>\r\n                <span class=\"navUser-item-searchLabel\">{{lang 'common.search'}}</span>\r\n            </a>\r\n        </li>\r\n        {{#if theme_settings.navPages-show-cart}}\r\n        <li class=\"navUser-item navUser-item--cart hide-desktop\">\r\n            <a\r\n                class=\"navUser-action\"\r\n                data-cart-preview\r\n                href=\"{{urls.cart}}\">\r\n                <span class=\"navUser-item-icon navUser-item-cartIcon emthemesModez-icon-linear-cart\"></span>\r\n                <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n            </a>\r\n        </li>\r\n        {{else}}\r\n        <li class=\"navUser-item navUser-item--cart\">\r\n            <a\r\n                class=\"navUser-action\"\r\n                data-cart-preview\r\n                data-dropdown=\"cart-preview-dropdown\"\r\n                data-options=\"align:right\"\r\n                href=\"{{urls.cart}}\">\r\n                <span class=\"navUser-item-icon navUser-item-cartIcon emthemesModez-icon-linear-cart\"></span>\r\n                <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n            </a>\r\n\r\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n        </li>\r\n        {{/if}}\r\n        <li class=\"navUser-item\">\r\n            <a class=\"navUser-action navUser-item--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\r\n        </li>\r\n        <li class=\"navUser-item navUser-item--account\">\r\n            {{#if customer}}\r\n                <a class=\"navUser-action\" href=\"{{urls.account.index}}\">\r\n                    <span class=\"navUser-item-icon navUser-item-accountIcon fa fa-user\"></span>\r\n                    <span class=\"navUser-item-accountLabel\">{{lang 'common.account'}}</span>\r\n                </a>\r\n                <a class=\"navUser-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\r\n            {{else}}\r\n                <a class=\"navUser-action\" href=\"{{urls.auth.login}}\">\r\n                    <span class=\"navUser-item-icon navUser-item-loginIcon fa fa-user\"></span>\r\n                    <span class=\"navUser-item-loginLabel\">{{lang 'common.login'}}</span>\r\n                </a>\r\n                {{!--\r\n                {{#if settings.account_creation_enabled}}\r\n                    <span class=\"navUser-or\">{{lang 'common.or'}}</span> <a class=\"navUser-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\r\n                {{/if}}\r\n                --}}\r\n            {{/if}}\r\n        </li>\r\n    </ul>\r\n    <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\r\n        {{> components/common/quick-search}}\r\n    </div>\r\n</nav>\r\n","components/common/quick-search":"<div class=\"container\">\n    {{{ snippet \"forms_search\"}}}\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n</div>\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                {{#if display_name '===' 'google+'}}\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\n                    </a>\n                {{else}}\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n"}
