{"components/common/compare-checkbox":"<input\n  type=\"checkbox\"\n  class=\"input-checkbox sr-only\"\n  name=\"products[]\"\n  value=\"{{id}}\"\n  id=\"compare-{{id}}\"\n  data-compare-checkbox\n  data-compare-id=\"{{id}}\"\n  data-compare-title=\"{{name}}\"\n  data-compare-url=\"{{url}}\"\n  data-compare-price=\"{{price.without_tax.formatted}}\"\n  data-compare-thumbnail=\"{{getImage image 'thumb' (cdn default_image)}}\">\n","components/products/price":"<div>\n  {{#if price.without_tax}}\n    <div class=\"product-price-line\" data-product-price-wrapper=\"without-tax\">\n      {{#if price.saved}}\n        <span class=\"price-rrp {{#unless price.with_tax}}highlight{{/unless}}\">{{price.rrp_without_tax.formatted}}</span>\n      {{/if}}\n\n      <span class=\"price-value\"> {{price.without_tax.formatted}}</span>\n\n      {{#if price.with_tax}}\n        <span class=\"tax-label\"> {{lang 'product.excluding_tax'}}</span>\n      {{/if}}\n    </div>\n  {{/if}}\n\n  {{#if price.with_tax}}\n    <div class=\"product-price-line{{#if price.without_tax}} price-second-line{{/if}}\" data-product-price-wrapper=\"with-tax\">\n      {{#if price.saved}}\n        <span class=\"price-rrp {{#unless price.without_tax}}highlight{{/unless}}\">{{price.rrp_with_tax.formatted}}</span>\n      {{/if}}\n\n      <span class=\"price-value\">{{price.with_tax.formatted}}</span>\n\n      {{#if price.without_tax}}\n        <span class=\"tax-label\"> {{lang 'product.including_tax'}}</span>\n      {{/if}}\n    </div>\n  {{/if}}\n\n  {{#if show_savings}}\n    {{#if price.saved}}\n      <div class=\"price-you-save\" data-product-price-saved>\n        {{lang 'product.you_save'}} {{price.saved.formatted}}\n      </div>\n    {{/if}}\n  {{/if}}\n</div>\n","components/products/product-list-item":"<article class=\"product-list-item product-block\" data-product-title=\"{{name}}\" data-product-id=\"{{id}}\">\n  <div class=\"product-thumb-wrapper\">\n    <figure class=\"product-item-thumbnail\">\n      {{#if price.saved}}\n        <div class=\"product-badge sale\"><span class=\"message\">{{lang 'product.sale'}}</span></div>\n      {{/if}}\n      {{#if out_of_stock_message}}\n        <div class=\"product-badge sold-out\"><span class=\"message\">{{out_of_stock_message}}</span></div>\n      {{/if}}\n      <a class=\"product-item-thumbnail-image\" href=\"{{url}}\" data-product-href=\"{{url}}\" style=\"background-image: url({{getImage image 'thumb' (cdn default_image)}})\">\n        <img class=\"sr-only\" src=\"{{getImage image 'thumb' (cdn default_image)}}\" alt=\"{{image.alt}}\" />\n      </a>\n    </figure>\n  </div>\n\n  <div class=\"product-item-details\">\n    {{#if show_rating}}\n      <p class=\"product-item-rating\">\n        {{> components/products/ratings rating=rating}}\n      </p>\n    {{/if}}\n\n    {{#if brand.name}}\n      <div class=\"product-item-brand\">{{brand.name}}</div>\n    {{/if}}\n    <h1 class=\"product-item-title\">\n      <a href=\"{{{url}}}\" alt=\"{{name}}\">{{name}}</a>\n    </h1>\n\n    <div class=\"product-item-price\" data-product-price=\"{{price.without_tax.formatted}}\">\n      {{> components/products/price price=price}}\n    </div>\n\n    <div class=\"product-description\">\n      {{{summary}}}\n    </div>\n\n    <div class=\"product-item-actions\">\n      {{#if show_compare}}\n        <label class=\"button button-short button-wide product-item-compare override-dropdown\" for=\"compare-{{id}}\">\n          {{> components/common/compare-checkbox}}\n          <span>\n            <svg class=\"icon icon-check-mark\"><use xlink:href=\"#icon-check-mark\" /></svg>\n            {{lang \"product.compare\"}}\n          </span>\n        </label>\n      {{/if}}\n\n      <button class=\"button button-secondary button-short button-wide\" data-quick-shop data-product-id=\"{{id}}\">\n        {{lang 'product.quick_view'}}\n      </button>\n    </div>\n  </div>\n</article>\n","components/products/ratings":"{{#for 1 5}}\n  <svg class=\"icon icon-star {{#if ../this.rating '>=' $index}}full{{/if}}\"><use xlink:href=\"#icon-star\" /></svg>\n{{/for}}\n"}
