{"components/products/options/input-checkbox":"<div class=\"form-field form-field-options form-field-checkbox{{#if required}} required{{/if}}\" data-product-attribute=\"input-checkbox\">\n  <div class=\"form-label-text product-option-title\">\n    {{display_name}}\n  </div>\n\n  <div class=\"form-field-control\">\n    <label class=\"form-label {{class}}\">\n      <input\n        class=\"form-input form-checkbox\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"checkbox\"\n        value=\"{{value}}\"\n        {{#if checked}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span>{{label}}</span>\n    </label>\n  </div>\n</div>\n"}
