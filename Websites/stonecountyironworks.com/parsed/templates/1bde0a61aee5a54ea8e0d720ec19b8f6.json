{"components/cart/configure-product":"{{> components/common/modal-close}}\n\n<div class=\"modal-header\">\n  <h2 class=\"title\">{{lang 'cart.reconfigure_product' name=product_name}}</h2>\n</div>\n\n<div class=\"modal-body product-options-container\" data-product-option-change>\n  <div data-cart-option-alerts></div>\n\n  <form class=\"cart-option-form\" method=\"post\" action=\"cart.php\" enctype=\"multipart/form-data\">\n    <input type=\"hidden\" name=\"action\" value=\"EditProductFieldsInCart\">\n    <input type=\"hidden\" name=\"item_id\" value=\"{{quote_item_id}}\">\n\n    {{#each customizations}}\n      {{{dynamicComponent 'components/products/customizations'}}}\n    {{/each}}\n\n    {{#each options}}\n      {{{dynamicComponent 'components/products/options'}}}\n    {{/each}}\n\n    <div class=\"form-actions\">\n      <button type=\"submit\" class=\"button button-primary button-save-options button-progress spinner\">\n        <span class=\"button-text\">{{lang 'cart.save_options'}}</span>\n      </button>\n    </div>\n  </form>\n</div>\n","components/common/modal-close":"<a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'cart.modal_close'}}\">\n  <span aria-hidden=\"true\">\n    <svg class=\"icon icon-close\"><use xlink:href=\"#icon-close\" /></svg>\n  </span>\n</a>\n","components/products/customizations/checkbox":"<div class=\"form-field form-inline {{#if required }}required{{/if}}\">\n  <label class=\"form-label\" for=\"customization_{{this.id}}_{{../id}}\">\n    <input class=\"form-input\" type=\"checkbox\" id=\"customization_{{this.id}}_{{../id}}\" name=\"ProductFields[{{this.id}}]\" {{#if checked}}checked{{/if}} {{#if required}}required{{/if}}>\n    <div class=\"form-label-text\">\n      {{display_name}}\n    </div>\n  </label>\n</div>\n","components/products/customizations/file":"{{> core/forms/customizations/file}}\n","components/products/customizations/select":"<div class=\"form-field {{#if required }}required{{/if}}\">\n  <label class=\"form-label\" for=\"customization_{{this.id}}_{{../id}}\">\n    <div class=\"form-label-text product-option-title\">\n      {{ display_name }}\n    </div>\n    <select class=\"form-input form-select\" id=\"customization_{{this.id}}_{{../id}}\" name=\"ProductFields[{{this.id}}]\">\n      {{#each values}}\n        <option value=\"{{this}}\">{{this}}</option>\n      {{/each}}\n    </select>\n  </label>\n</div>\n","components/products/customizations/text":"<div class=\"form-field {{#if required }}required{{/if}}\">\n  <label class=\"form-label\" for=\"customization_{{this.id}}_{{../id}}\">\n    <div class=\"form-label-text product-option-title\">\n      {{ display_name }}\n    </div>\n    <input type=\"text\" class=\"form-input\" id=\"customization_{{this.id}}_{{../id}}\" name=\"ProductFields[{{this.id}}]\" placeholder=\"{{prefill}}\" value=\"{{value}}\">\n  </label>\n</div>\n","components/products/customizations/textarea":"<div class=\"form-field {{#if required }}required{{/if}}\">\n  <label class=\"form-label\" for=\"customization_{{this.id}}_{{../id}}\">\n    <div class=\"form-label-text product-option-title\">\n      {{display_name}}\n    </div>\n    <textarea class=\"form-input\" id=\"customization_{{this.id}}_{{../id}}\" name=\"ProductFields[{{this.id}}]\" class=\"form-input\" rows=\"4\" placeholder=\"{{prefill}}\"></textarea>\n  </label>\n</div>\n","components/products/options/date":"\n<div class=\"form-field  {{#if required}}required{{/if}}\" data-product-attribute=\"date\">\n  <label class=\"form-label\">\n\n    <div class=\"form-label-text product-option-title\">\n      {{display_name}}\n    </div>\n    <div class=\"date-select-group\">\n      <div class=\"form-select-wrapper month\">\n        <select name=\"attribute[{{id}}][month]\" class=\"form-input form-select\">\n          <option disabled>{{lang 'date.month'}}</option>\n          {{#enumerate 1 12}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.month this}}selected=\"selected\"{{/equals}}>{{getShortMonth this}}</option>\n          {{/enumerate}}\n        </select>\n      </div>\n\n      <div class=\"form-select-wrapper day\">\n        <select name=\"attribute[{{id}}][day]\" class=\"form-input form-select\">\n          <option disabled>{{lang 'date.day'}}</option>\n          {{#enumerate 1 31}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.day this}}selected=\"selected\"{{/equals}}>{{this}}</option>\n          {{/enumerate}}\n        </select>\n      </div>\n\n      <div class=\"form-select-wrapper year\">\n        <select name=\"attribute[{{id}}][year]\" class=\"form-input form-select\">\n          <option disabled>{{lang 'date.year'}}</option>\n          {{#enumerate earliest_year latest_year}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.year this}}selected=\"selected\"{{/equals}}>{{this}}</option>\n          {{/enumerate}}\n        </select>\n      </div>\n    </div>\n\n  </label>\n</div>\n","components/products/options/input-checkbox":"<div class=\"form-field form-field-options form-field-checkbox{{#if required}} required{{/if}}\" data-product-attribute=\"input-checkbox\">\n  <div class=\"form-label-text product-option-title\">\n    {{display_name}}\n  </div>\n\n  <div class=\"form-field-control\">\n    <label class=\"form-label {{class}}\">\n      <input\n        class=\"form-input form-checkbox\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"checkbox\"\n        value=\"{{value}}\"\n        {{#if checked}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span>{{label}}</span>\n    </label>\n  </div>\n</div>\n","components/products/options/input-file":"{{> core/forms/options/input-file}}\n","components/products/options/input-numbers":"\n<div class=\"form-field {{#if required }}required{{/if}}\" data-product-attribute=\"input-number\">\n  <label class=\"form-label\">\n    <div class=\"form-label-text product-option-title\">\n      {{display_name}}\n    </div>\n    <input type=\"number\" class=\"form-input input-number\" name=\"attribute[{{id}}]\" value=\"{{prefill}}\" step=\"any\">\n  </label>\n</div>\n","components/products/options/input-text":"\n<div class=\"form-field {{#if required }}required{{/if}}\" data-product-attribute=\"input-text\">\n  <label class=\"form-label\">\n    <div class=\"form-label-text product-option-title\">\n      {{ display_name }}\n    </div>\n    <input type=\"text\" class=\"form-input\" name=\"attribute[{{id}}]\" placeholder=\"{{prefill}}\" value=\"\">\n  </label>\n</div>\n","components/products/options/product-list":"<div class=\"form-field {{#if required }}required{{/if}}\" data-product-attribute=\"product-list\">\n  <label class=\"form-label-text product-option-title\">\n    {{display_name}}\n  </label>\n\n  {{#unless values.1.image}}\n\n    {{!-- same as radio buttons if no image set --}}\n    <div class=\"form-inline\">\n      {{#each values}}\n        <label class=\"form-label radio\" for=\"attribute_{{id}}_{{../id}}\">\n          <input type=\"radio\" class=\"form-input\" name=\"attribute[{{../id}}]\" id=\"attribute_{{id}}_{{../id}}\" value=\"{{id}}\" {{#if selected}}checked{{/if}}>\n          <span class=\"input-label\">{{label}}</span>\n        </label>\n      {{/each}}\n      <label class=\"form-label radio\" for=\"attribute_0_{{id}}\">\n        <input type=\"radio\" class=\"form-input\" name=\"attribute[{{id}}]\" value=\"0\" id=\"attribute_0_{{id}}\" {{#if defaultValue '==' 0}}checked{{/if}}>\n        <span class=\"input-label\">{{lang 'product.none'}}</span>\n      </label>\n    </div>\n\n  {{else}}\n\n    <ul class=\"product-picklist\">\n      {{#each values}}\n        <li class=\"product-picklist-item\">\n          <input class=\"product-picklist-radio\" type=\"radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" id=\"attribute_{{id}}_{{../id}}\" {{#if selected}}checked{{/if}} {{#if ../required}}required{{/if}}>\n          <label class=\"product-picklist-block\" for=\"attribute_{{id}}_{{../id}}\">\n            {{#if image}}\n              <div class=\"product-picklist-image-container\">\n                <img class=\"product-picklist-image\" src=\"{{getImage image 'thumb'}}\" alt=\"{{image.alt}}\">\n              </div>\n            {{/if}}\n            <span class=\"product-picklist-label\">{{label}}</span>\n          </label>\n        </li>\n      {{/each}}\n      <li class=\"product-picklist-item\">\n        <input class=\"product-picklist-radio\" type=\"radio\" name=\"attribute[{{id}}]\" value=\"0\" id=\"attribute_0_{{id}}\" {{#if defaultValue '==' 0}}checked{{/if}} {{#if required}}required{{/if}}>\n        <label class=\"product-picklist-block picklist-none\" for=\"attribute_0_{{id}}\">\n          <span class=\"product-picklist-label\">{{lang 'product.none'}}</span>\n        </label>\n      </li>\n    </ul>\n\n  {{/unless}}\n</div>\n","components/products/options/set-radio":"\n<div class=\"form-field {{#if required }}required{{/if}}\" data-product-attribute=\"set-radio\">\n  <div class=\"product-option-title\">\n    {{display_name}}\n  </div>\n  <div class=\"form-inline\">\n    {{#each values}}\n      <label class=\"form-label radio\" data-product-attribute-value=\"{{id}}\">\n        <input type=\"radio\" class=\"form-input\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" {{#if selected}}checked{{/if}}>\n        <span class=\"form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n","components/products/options/set-rectangle":"\n<div class=\"form-field set-rectangle {{#if required }}required{{/if}}\" data-product-attribute=\"set-rectangle\">\n  <div class=\"product-option-title\">\n    {{display_name}}\n  </div>\n  <div class=\"form-rectangles\">\n    {{#each values}}\n      <label class=\"form-label rectangle\" data-product-attribute-value=\"{{id}}\">\n        <input type=\"radio\" name=\"attribute[{{../id}}]\" id=\"attribute-{{id}}\" value=\"{{id}}\" {{#if selected}}checked{{/if}}>\n        <span class=\"rectangle-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n","components/products/options/set-select":"\n<div class=\"form-field {{#if required }}required{{/if}}\" data-product-attribute=\"set-select\">\n  <label class=\"form-label\">\n    <div class=\"form-label-text product-option-title\">\n      {{display_name}}\n    </div>\n    <div class=\"form-select-wrapper\">\n      <select name=\"attribute[{{id}}]\" class=\"form-input form-select\">\n        {{#each values}}\n          <option value=\"{{id}}\" {{#if selected}}selected{{/if}} data-product-attribute-value=\"{{id}}\">{{label}}</option>\n        {{/each}}\n      </select>\n    </div>\n  </label>\n</div>\n","components/products/options/swatch":"<div class=\"form-field swatch-group {{#if required }}required{{/if}}\" data-product-attribute=\"swatch\">\r\n  <div class=\"form-label-text product-option-title\">\r\n    {{display_name}}\r\n    <span class=\"swatch-value\"></span>\r\n  </div>\r\n\r\n  <div class=\"swatches\">\r\n    {{#each values}}\r\n      <label class=\"swatch-wrap\" data-swatch-value=\"{{label}}\" data-product-attribute-value=\"{{id}}\">\r\n        <span class=\"sr-only\">{{label}}</span>\r\n\r\n        <input type=\"radio\" class=\"swatch-radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" {{#if selected}}checked{{/if}}>\r\n\r\n        <div class=\"swatch\">\r\n          <svg class=\"icon icon-check-mark\"><use xlink:href=\"#icon-check-mark\" /></svg>\r\n          {{#if pattern}}\r\n            <div class=\"swatch-color\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></div>\r\n          {{/if}}\r\n\r\n          {{#if data.[0]}}\r\n            <div class=\"swatch-color\" style=\"background-color: #{{data.[0]}}\"></div>\r\n          {{/if}}\r\n\r\n          {{#if data.[1]}}\r\n            <div class=\"swatch-color secondary\" style=\"background-color: #{{data.[1]}}\"></div>\r\n          {{/if}}\r\n\r\n          {{#if data.[2]}}\r\n            <div class=\"swatch-color tertiary\" style=\"background-color: #{{data.[2]}}\"></div>\r\n          {{/if}}\r\n        </div>\r\n\r\n        {{#if pattern}}\r\n          <span class=\"swatch-pattern-expanded\">\r\n            <span class=\"swatch-pattern-image\" style=\"background-image: url('{{getImage image}}');\">\r\n                \r\n          </span>\r\n          {{label}}\r\n          </span>\r\n          \r\n        {{/if}}\r\n\r\n      </label>\r\n    {{/each}}\r\n  </div>\r\n</div>\r\n","components/products/options/textarea":"<div class=\"form-field {{#if required }}required{{/if}}\" data-product-attribute=\"textarea\">\n  <label class=\"form-label\">\n    <div class=\"form-label-text product-option-title\">\n      {{display_name}}\n    </div>\n    <textarea name=\"attribute[{{id}}]\" class=\"form-input\" rows=\"4\" placeholder=\"{{prefill}}\"></textarea>\n  </label>\n</div>\n","core/forms/customizations/file":"\n<div class=\"form-field form-field-customizations form-field-file {{#if required}}form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n","core/forms/options/input-file":"\n<div class=\"form-field form-field-options form-field-file {{#if required}}form-required{{/if}}\"\n  data-product-attribute=\"input-file\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n"}
