{"components/common/pagination":"{{#if links.length}}\n  <nav class=\"pagination\">\n    <a\n      class=\"pagination-link {{#unless previous}}disabled{{/unless}}\"\n      href=\"{{previous}}\">\n        <svg class=\"icon icon-arrow-left\"><use xlink:href=\"#icon-arrow-left\" /></svg>\n    </a>\n\n    {{#each links}}\n      <a\n        class=\"pagination-link pagination-page-link {{#if ../current \"==\" number}}active{{/if}}\"\n        href=\"{{url}}\">\n          {{number}}\n      </a>\n    {{/each}}\n\n    <a\n      class=\"pagination-link {{#unless next}}disabled{{/unless}}\"\n      href=\"{{next}}\">\n        <svg class=\"icon icon-arrow-right\"><use xlink:href=\"#icon-arrow-right\" /></svg>\n    </a>\n  </nav>\n{{/if}}\n"}
