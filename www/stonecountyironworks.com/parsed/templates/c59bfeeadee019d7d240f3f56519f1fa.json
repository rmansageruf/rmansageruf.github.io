{"components/cart/shipping-quote":"<li class=\"form-field form-inline\">\n  <label class=\"shipping-quote-item\" for=\"shipping-quote-{{id}}\">\n    <input id=\"shipping-quote-{{id}}\" class=\"form-input input-radio\" type=\"radio\" name=\"shipping_method\" value=\"{{id}}\" data-shipping-quote>\n    <span class=\"form-label-text\">\n      {{#if logo_path}}<img class=\"shipping-logo\" src=\"{{logo_path}}\" alt=\"{{provider_name}}\" />{{/if}}\n      {{provider_name}}\n    </span>\n    <span class=\"cost\">{{cost.formatted}}</span>\n  </label>\n</li>\n","components/cart/shipping-quotes":"<h5>{{lang 'cart.shipping_calculator.methods'}}</h5>\n<form class=\"form\">\n  <ul class=\"shipping-quote-list\">\n    {{#if quotes.shipping_methods}}\n      {{#each quotes.shipping_methods}}\n        {{> components/cart/shipping-quote}}\n      {{/each}}\n    {{/if}}\n\n    {{#if quotes.ups_shipping_methods}}\n      {{#each quotes.ups_shipping_methods}}\n        {{> components/cart/shipping-quote provider_name=description}}\n      {{/each}}\n    {{/if}}\n  </ul>\n  <div class=\"form-actions\">\n    <button class=\"button button-wide button-progress spinner button-apply-shipping\" type=\"submit\">\n      <span class=\"button-text\">{{lang 'cart.shipping_calculator.apply'}}</span>\n    </button>\n    <button class=\"button button-secondary button-wide\" type=\"button\" data-cancel-shipping>{{lang 'common.form_cancel'}}</button>\n  </div>\n</form>\n"}
