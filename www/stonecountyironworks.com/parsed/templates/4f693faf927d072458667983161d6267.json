{"components/common/quantity":"{{!\n  Quantity Widget with Increment/Decrement controls.\n\n  @param product\n         cart.item or product object\n}}\n\n<div class=\"quantity-control\" data-quantity-control=\"{{id}}\">\n  <input\n    class=\"form-input\"\n    type=\"number\"\n    name=\"qty{{#if product_id}}-{{id}}{{else}}[]{{/if}}\"\n    pattern=\"[0-9]*\"\n    value=\"{{#if quantity}}{{quantity}}{{else}}{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}{{/if}}\"\n    min=\"{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}0{{/if}}\"\n    max=\"{{#if max_purchase_quantity}}{{max_purchase_quantity}}{{/if}}\"\n    data-quantity-control-input\n  >\n  <button class=\"button-control-action\" data-quantity-control-action=\"increment\">\n    &#43;\n  </button>\n  <button class=\"button-control-action\" data-quantity-control-action=\"decrement\">\n    &#8211;\n  </button>\n</div>\n","components/common/share-buttons":"{{#if share_buttons}}\n  <nav class=\"share-buttons-nav\">\n    <ul class=\"share-buttons\" data-share-buttons>\n\n      {{#each share_buttons.buttons}}\n        {{#if service '===' 'facebook_like'}}\n          <li class=\"social-link facebook-like\">\n            <div class=\"fb-like\"\n              data-href=\"{{../../url}}\"\n              data-layout=\"button_count\"\n              data-action=\"like\"\n              data-show-faces=\"false\">\n            </div>\n          </li>\n        {{/if}}\n      {{/each}}\n\n      {{#each share_buttons.buttons}}\n        {{#if service '===' 'facebook'}}\n          <li class=\"social-link facebook\">\n            <a target=\"_blank\" href=\"http://www.facebook.com/sharer.php?u={{../../url}}\" class=\"facebook\">\n              {{> svg/social/facebook}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'twitter'}}\n          <li class=\"social-link twitter\">\n            <a target=\"_blank\" href=\"https://twitter.com/share?url={{../../url}}&amp;text={{../../title}}\" class=\"twitter\">\n              {{> svg/social/twitter}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'google_plusone'}}\n          <li class=\"social-link google-plus\">\n            <a target=\"_blank\" href=\"https://plus.google.com/share?url={{../../url}}\" class=\"google\">\n              {{> svg/social/googleplus}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'linkedin'}}\n          <li class=\"social-link linkedin\">\n            <a target=\"_blank\" href=\"http://www.linkedin.com/shareArticle?url={{../../url}}&amp;title={{../../title}}\" class=\"linkedin\">\n              {{> svg/social/linkedin}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'email'}}\n          <li class=\"social-link mail\">\n            <a href=\"mailto:?subject={{../../title}}&amp;body={{../../url}}\" class=\"email\">\n              {{> svg/social/email}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'print'}}\n          <li class=\"social-link print\">\n            <a class=\"print\" href=\"javascript:;\" onclick=\"window.print()\">\n              {{> svg/social/print}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'pinterest'}}\n          <li class=\"social-link pinterest\">\n            <a target=\"_blank\" href=\"//pinterest.com/pin/create/button/?url={{../../url}}&amp;media={{../../image}}&amp;description={{../../title}}\" class=\"share-pinterest\">\n              {{> svg/social/pinterest}}\n            </a>\n          </li>\n        {{/if}}\n      {{/each}}\n\n    </ul>\n  </nav>\n{{/if}}\n","components/products/bulk-pricing":"{{#if product.bulk_discount_rates.length}}\n  <div class=\"bulk-pricing-block\">\n    <h1 class=\"product-details-heading\">{{lang 'product.bulk_pricing.title'}}</h1>\n    <ul>\n      {{#each product.bulk_discount_rates}}\n        <li>\n          {{lang 'product.bulk_pricing.range' min=min max=max}}\n          {{#if type '===' 'percent'}}\n            {{lang 'product.bulk_pricing.percent' discount=discount.formatted}}\n          {{/if}}\n          {{#if type '===' 'fixed'}}\n            {{lang 'product.bulk_pricing.fixed' discount=discount.formatted}}\n          {{/if}}\n          {{#if type '===' 'price'}}\n            {{lang 'product.bulk_pricing.price' discount=discount.formatted}}\n          {{/if}}\n        </li>\n      {{/each}}\n    </ul>\n  </div>\n{{/if}}\n","components/products/customizations/checkbox":"<div class=\"form-field form-inline {{#if required }}required{{/if}}\">\n  <label class=\"form-label\" for=\"customization_{{this.id}}_{{../id}}\">\n    <input class=\"form-input\" type=\"checkbox\" id=\"customization_{{this.id}}_{{../id}}\" name=\"ProductFields[{{this.id}}]\" {{#if checked}}checked{{/if}} {{#if required}}required{{/if}}>\n    <div class=\"form-label-text\">\n      {{display_name}}\n    </div>\n  </label>\n</div>\n","components/products/customizations/file":"{{> core/forms/customizations/file}}\n","components/products/customizations/select":"<div class=\"form-field {{#if required }}required{{/if}}\">\n  <label class=\"form-label\" for=\"customization_{{this.id}}_{{../id}}\">\n    <div class=\"form-label-text product-option-title\">\n      {{ display_name }}\n    </div>\n    <select class=\"form-input form-select\" id=\"customization_{{this.id}}_{{../id}}\" name=\"ProductFields[{{this.id}}]\">\n      {{#each values}}\n        <option value=\"{{this}}\">{{this}}</option>\n      {{/each}}\n    </select>\n  </label>\n</div>\n","components/products/customizations/text":"<div class=\"form-field {{#if required }}required{{/if}}\">\n  <label class=\"form-label\" for=\"customization_{{this.id}}_{{../id}}\">\n    <div class=\"form-label-text product-option-title\">\n      {{ display_name }}\n    </div>\n    <input type=\"text\" class=\"form-input\" id=\"customization_{{this.id}}_{{../id}}\" name=\"ProductFields[{{this.id}}]\" placeholder=\"{{prefill}}\" value=\"{{value}}\">\n  </label>\n</div>\n","components/products/customizations/textarea":"<div class=\"form-field {{#if required }}required{{/if}}\">\n  <label class=\"form-label\" for=\"customization_{{this.id}}_{{../id}}\">\n    <div class=\"form-label-text product-option-title\">\n      {{display_name}}\n    </div>\n    <textarea class=\"form-input\" id=\"customization_{{this.id}}_{{../id}}\" name=\"ProductFields[{{this.id}}]\" class=\"form-input\" rows=\"4\" placeholder=\"{{prefill}}\"></textarea>\n  </label>\n</div>\n","components/products/options/date":"\n<div class=\"form-field  {{#if required}}required{{/if}}\" data-product-attribute=\"date\">\n  <label class=\"form-label\">\n\n    <div class=\"form-label-text product-option-title\">\n      {{display_name}}\n    </div>\n    <div class=\"date-select-group\">\n      <div class=\"form-select-wrapper month\">\n        <select name=\"attribute[{{id}}][month]\" class=\"form-input form-select\">\n          <option disabled>{{lang 'date.month'}}</option>\n          {{#enumerate 1 12}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.month this}}selected=\"selected\"{{/equals}}>{{getShortMonth this}}</option>\n          {{/enumerate}}\n        </select>\n      </div>\n\n      <div class=\"form-select-wrapper day\">\n        <select name=\"attribute[{{id}}][day]\" class=\"form-input form-select\">\n          <option disabled>{{lang 'date.day'}}</option>\n          {{#enumerate 1 31}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.day this}}selected=\"selected\"{{/equals}}>{{this}}</option>\n          {{/enumerate}}\n        </select>\n      </div>\n\n      <div class=\"form-select-wrapper year\">\n        <select name=\"attribute[{{id}}][year]\" class=\"form-input form-select\">\n          <option disabled>{{lang 'date.year'}}</option>\n          {{#enumerate earliest_year latest_year}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.year this}}selected=\"selected\"{{/equals}}>{{this}}</option>\n          {{/enumerate}}\n        </select>\n      </div>\n    </div>\n\n  </label>\n</div>\n","components/products/options/input-checkbox":"<div class=\"form-field form-field-options form-field-checkbox{{#if required}} required{{/if}}\" data-product-attribute=\"input-checkbox\">\n  <div class=\"form-label-text product-option-title\">\n    {{display_name}}\n  </div>\n\n  <div class=\"form-field-control\">\n    <label class=\"form-label {{class}}\">\n      <input\n        class=\"form-input form-checkbox\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"checkbox\"\n        value=\"{{value}}\"\n        {{#if checked}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span>{{label}}</span>\n    </label>\n  </div>\n</div>\n","components/products/options/input-file":"{{> core/forms/options/input-file}}\n","components/products/options/input-numbers":"\n<div class=\"form-field {{#if required }}required{{/if}}\" data-product-attribute=\"input-number\">\n  <label class=\"form-label\">\n    <div class=\"form-label-text product-option-title\">\n      {{display_name}}\n    </div>\n    <input type=\"number\" class=\"form-input input-number\" name=\"attribute[{{id}}]\" value=\"{{prefill}}\" step=\"any\">\n  </label>\n</div>\n","components/products/options/input-text":"\n<div class=\"form-field {{#if required }}required{{/if}}\" data-product-attribute=\"input-text\">\n  <label class=\"form-label\">\n    <div class=\"form-label-text product-option-title\">\n      {{ display_name }}\n    </div>\n    <input type=\"text\" class=\"form-input\" name=\"attribute[{{id}}]\" placeholder=\"{{prefill}}\" value=\"\">\n  </label>\n</div>\n","components/products/options/product-list":"<div class=\"form-field {{#if required }}required{{/if}}\" data-product-attribute=\"product-list\">\n  <label class=\"form-label-text product-option-title\">\n    {{display_name}}\n  </label>\n\n  {{#unless values.1.image}}\n\n    {{!-- same as radio buttons if no image set --}}\n    <div class=\"form-inline\">\n      {{#each values}}\n        <label class=\"form-label radio\" for=\"attribute_{{id}}_{{../id}}\">\n          <input type=\"radio\" class=\"form-input\" name=\"attribute[{{../id}}]\" id=\"attribute_{{id}}_{{../id}}\" value=\"{{id}}\" {{#if selected}}checked{{/if}}>\n          <span class=\"input-label\">{{label}}</span>\n        </label>\n      {{/each}}\n      <label class=\"form-label radio\" for=\"attribute_0_{{id}}\">\n        <input type=\"radio\" class=\"form-input\" name=\"attribute[{{id}}]\" value=\"0\" id=\"attribute_0_{{id}}\" {{#if defaultValue '==' 0}}checked{{/if}}>\n        <span class=\"input-label\">{{lang 'product.none'}}</span>\n      </label>\n    </div>\n\n  {{else}}\n\n    <ul class=\"product-picklist\">\n      {{#each values}}\n        <li class=\"product-picklist-item\">\n          <input class=\"product-picklist-radio\" type=\"radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" id=\"attribute_{{id}}_{{../id}}\" {{#if selected}}checked{{/if}} {{#if ../required}}required{{/if}}>\n          <label class=\"product-picklist-block\" for=\"attribute_{{id}}_{{../id}}\">\n            {{#if image}}\n              <div class=\"product-picklist-image-container\">\n                <img class=\"product-picklist-image\" src=\"{{getImage image 'thumb'}}\" alt=\"{{image.alt}}\">\n              </div>\n            {{/if}}\n            <span class=\"product-picklist-label\">{{label}}</span>\n          </label>\n        </li>\n      {{/each}}\n      <li class=\"product-picklist-item\">\n        <input class=\"product-picklist-radio\" type=\"radio\" name=\"attribute[{{id}}]\" value=\"0\" id=\"attribute_0_{{id}}\" {{#if defaultValue '==' 0}}checked{{/if}} {{#if required}}required{{/if}}>\n        <label class=\"product-picklist-block picklist-none\" for=\"attribute_0_{{id}}\">\n          <span class=\"product-picklist-label\">{{lang 'product.none'}}</span>\n        </label>\n      </li>\n    </ul>\n\n  {{/unless}}\n</div>\n","components/products/options/set-radio":"\n<div class=\"form-field {{#if required }}required{{/if}}\" data-product-attribute=\"set-radio\">\n  <div class=\"product-option-title\">\n    {{display_name}}\n  </div>\n  <div class=\"form-inline\">\n    {{#each values}}\n      <label class=\"form-label radio\" data-product-attribute-value=\"{{id}}\">\n        <input type=\"radio\" class=\"form-input\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" {{#if selected}}checked{{/if}}>\n        <span class=\"form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n","components/products/options/set-rectangle":"\n<div class=\"form-field set-rectangle {{#if required }}required{{/if}}\" data-product-attribute=\"set-rectangle\">\n  <div class=\"product-option-title\">\n    {{display_name}}\n  </div>\n  <div class=\"form-rectangles\">\n    {{#each values}}\n      <label class=\"form-label rectangle\" data-product-attribute-value=\"{{id}}\">\n        <input type=\"radio\" name=\"attribute[{{../id}}]\" id=\"attribute-{{id}}\" value=\"{{id}}\" {{#if selected}}checked{{/if}}>\n        <span class=\"rectangle-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n","components/products/options/set-select":"\n<div class=\"form-field {{#if required }}required{{/if}}\" data-product-attribute=\"set-select\">\n  <label class=\"form-label\">\n    <div class=\"form-label-text product-option-title\">\n      {{display_name}}\n    </div>\n    <div class=\"form-select-wrapper\">\n      <select name=\"attribute[{{id}}]\" class=\"form-input form-select\">\n        {{#each values}}\n          <option value=\"{{id}}\" {{#if selected}}selected{{/if}} data-product-attribute-value=\"{{id}}\">{{label}}</option>\n        {{/each}}\n      </select>\n    </div>\n  </label>\n</div>\n","components/products/options/swatch":"<div class=\"form-field swatch-group {{#if required }}required{{/if}}\" data-product-attribute=\"swatch\">\r\n  <div class=\"form-label-text product-option-title\">\r\n    {{display_name}}\r\n    <span class=\"swatch-value\"></span>\r\n  </div>\r\n\r\n  <div class=\"swatches\">\r\n    {{#each values}}\r\n      <label class=\"swatch-wrap\" data-swatch-value=\"{{label}}\" data-product-attribute-value=\"{{id}}\">\r\n        <span class=\"sr-only\">{{label}}</span>\r\n\r\n        <input type=\"radio\" class=\"swatch-radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" {{#if selected}}checked{{/if}}>\r\n\r\n        <div class=\"swatch\">\r\n          <svg class=\"icon icon-check-mark\"><use xlink:href=\"#icon-check-mark\" /></svg>\r\n          {{#if pattern}}\r\n            <div class=\"swatch-color\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></div>\r\n          {{/if}}\r\n\r\n          {{#if data.[0]}}\r\n            <div class=\"swatch-color\" style=\"background-color: #{{data.[0]}}\"></div>\r\n          {{/if}}\r\n\r\n          {{#if data.[1]}}\r\n            <div class=\"swatch-color secondary\" style=\"background-color: #{{data.[1]}}\"></div>\r\n          {{/if}}\r\n\r\n          {{#if data.[2]}}\r\n            <div class=\"swatch-color tertiary\" style=\"background-color: #{{data.[2]}}\"></div>\r\n          {{/if}}\r\n        </div>\r\n\r\n        {{#if pattern}}\r\n          <span class=\"swatch-pattern-expanded\">\r\n            <span class=\"swatch-pattern-image\" style=\"background-image: url('{{getImage image}}');\">\r\n                \r\n          </span>\r\n          {{label}}\r\n          </span>\r\n          \r\n        {{/if}}\r\n\r\n      </label>\r\n    {{/each}}\r\n  </div>\r\n</div>\r\n","components/products/options/textarea":"<div class=\"form-field {{#if required }}required{{/if}}\" data-product-attribute=\"textarea\">\n  <label class=\"form-label\">\n    <div class=\"form-label-text product-option-title\">\n      {{display_name}}\n    </div>\n    <textarea name=\"attribute[{{id}}]\" class=\"form-input\" rows=\"4\" placeholder=\"{{prefill}}\"></textarea>\n  </label>\n</div>\n","components/products/price":"<div>\n  {{#if price.without_tax}}\n    <div class=\"product-price-line\" data-product-price-wrapper=\"without-tax\">\n      {{#if price.saved}}\n        <span class=\"price-rrp {{#unless price.with_tax}}highlight{{/unless}}\">{{price.rrp_without_tax.formatted}}</span>\n      {{/if}}\n\n      <span class=\"price-value\"> {{price.without_tax.formatted}}</span>\n\n      {{#if price.with_tax}}\n        <span class=\"tax-label\"> {{lang 'product.excluding_tax'}}</span>\n      {{/if}}\n    </div>\n  {{/if}}\n\n  {{#if price.with_tax}}\n    <div class=\"product-price-line{{#if price.without_tax}} price-second-line{{/if}}\" data-product-price-wrapper=\"with-tax\">\n      {{#if price.saved}}\n        <span class=\"price-rrp {{#unless price.without_tax}}highlight{{/unless}}\">{{price.rrp_with_tax.formatted}}</span>\n      {{/if}}\n\n      <span class=\"price-value\">{{price.with_tax.formatted}}</span>\n\n      {{#if price.without_tax}}\n        <span class=\"tax-label\"> {{lang 'product.including_tax'}}</span>\n      {{/if}}\n    </div>\n  {{/if}}\n\n  {{#if show_savings}}\n    {{#if price.saved}}\n      <div class=\"price-you-save\" data-product-price-saved>\n        {{lang 'product.you_save'}} {{price.saved.formatted}}\n      </div>\n    {{/if}}\n  {{/if}}\n</div>\n","components/products/product-single-details":"<section class=\"product-details product-block section {{#unless quick_view}}product-single{{/unless}}\" data-product-container data-product-title=\"{{product.title}}\" data-product-id=\"{{product.id}}\">\r\n\r\n  <div class=\"container\">\r\n    <div class=\"product-details-column {{#if product.images}}has-images{{/if}}\">\r\n      {{> components/products/product-single-images images=product.images}}\r\n    </div>\r\n\r\n    <div class=\"product-details-column {{#if product.images}}has-images{{/if}}\">\r\n      {{#if product.brand}}\r\n        <a class=\"product-brand\" href=\"{{product.brand.url}}\">{{product.brand.name}}</a>\r\n      {{/if}}\r\n\r\n      {{#if quick_view}}\r\n        <h1 class=\"product-title\"><a href=\"{{product.url}}\">{{this.product.title}}</a></h1>\r\n      {{else}}\r\n        <h1 class=\"product-title\">{{this.product.title}}</h1>\r\n      {{/if}}\r\n\r\n      <div class=\"product-item-price\">\r\n        {{#if product.can_purchase}}\r\n          {{> components/products/price price=product.price show_savings=true save_block=true schema_org=true}}\r\n        {{/if}}\r\n\r\n        {{#if product.call_for_price}}\r\n          <p>{{product.call_for_price}}</p>\r\n        {{/if}}\r\n      </div>\r\n    \r\n    \r\n     {{#each links}}\r\n    {{#if display_name '==' 'stumbleupon'}}\r\n      <div class=\"social-media-link\">\r\n        <a href=\"{{this.url}}\" title=\"{{this.name}}\" target=\"_blank\">\r\n          {{> svg/social/stumbleupon}}\r\n          <span class=\"sr-only\">{{this.name}}</span>\r\n        </a>\r\n      </div>\r\n    {{/if}}\r\n      {{/each}}\r\n    <div ID=\"product_msg\"></div>\r\n\r\n\r\n\r\n     {{#if product.sku}}\r\n     <div ID=\"productsku\">\r\n          <dl class=\"product-details-block product-details-sku\">\r\n            <dt class=\"product-details-name\">{{lang 'product.sku'}}:</dt>\r\n            <dd class=\"product-details-value\" data-product-sku>{{product.sku}}</dd>\r\n          </dl></div>\r\n        {{/if}}\r\n        \r\n        \r\n      <div class=\"product-rating-block\">\r\n        {{#if settings.show_product_rating}}\r\n          <div class=\"product-item-rating\">\r\n            {{> components/products/ratings rating=this.product.rating}}\r\n            <span class=\"total-reviews reviews-jumplink\" data-scroll=\"#product-reviews\">\r\n              <a class=\"product-reviews-link\" href=\"{{#if quick_view}}{{product.url}}{{/if}}#product-reviews\">{{lang 'product.reviews.title' numReviews=product.num_reviews}}</a>\r\n            </span>\r\n          </div>\r\n\r\n          {{{snippet 'product_rating'}}}\r\n        {{/if}}\r\n      </div>\r\n\r\n      {{#if product.release_date }}\r\n        <p class=\"message neutral\">{{product.release_date}}</p>\r\n      {{/if}}\r\n\r\n      <form method=\"post\" id=\"product-add-cart\" action=\"{{product.cart_url}}\" enctype=\"multipart/form-data\" data-cart-item-add>\r\n        <input type=\"hidden\" name=\"action\" value=\"add\">\r\n        <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" data-product-id/>\r\n\r\n        {{#if product.customizations.length}}\r\n          <div class=\"product-options-container\">\r\n            {{#each product.customizations}}\r\n              {{{dynamicComponent 'components/products/customizations'}}}\r\n            {{/each}}\r\n          </div>\r\n        {{/if}}\r\n\r\n        {{#if product.options.length}}\r\n          <div class=\"product-options-container\" data-product-option-change>\r\n            {{#each product.options}}\r\n              {{{dynamicComponent 'components/products/options'}}}\r\n            {{/each}}\r\n          </div>\r\n        {{/if}}\r\n\r\n        {{> components/products/bulk-pricing}}\r\n\r\n        <div class=\"product-stock {{#unless product.show_quantity_input}}default-layout{{/unless}}\">\r\n          <span class=\"{{#unless product.stock_level}}product-details-hidden{{/unless}}\" data-product-stock>{{lang 'product.current_stock'}}</span>\r\n          <span class=\"{{#unless product.stock_level}}product-details-hidden{{/unless}}\" data-product-stock data-product-stock-level>{{#if product.stock_level}}{{product.stock_level}}{{else}}0{{/if}}</span>\r\n\r\n          {{#if product.out_of_stock}}\r\n            <span class=\"stock-message\">\r\n            {{#if product.out_of_stock_message}}\r\n              {{product.out_of_stock_message}}\r\n            {{else}}\r\n              {{lang 'product.sold_out'}}\r\n            {{/if}}\r\n            <span/>\r\n          {{/if}}\r\n        </div>\r\n\r\n    <div class=\"form-label-text product-option-title\">Price As Configured</div>\r\n    <div class=\"product-item-price\">\r\n        {{#if product.can_purchase}}\r\n          {{> components/products/price price=product.price show_savings=true save_block=true schema_org=true}}\r\n        {{/if}}\r\n\r\n        {{#if product.call_for_price}}\r\n          <p>{{product.call_for_price}}</p>\r\n        {{/if}}\r\n      </div>\r\n\r\n        {{#if product.show_quantity_input}}\r\n          <div class=\"product-quantity-container\">\r\n            <div class=\"form-field\">\r\n              <label class=\"form-label\">\r\n                <span class=\"form-label-text product-option-title\">{{ lang 'product.quantity'}}</span>\r\n                {{> components/common/quantity product}}\r\n              </label>\r\n            </div>\r\n          </div>\r\n        {{/if}}\r\n\r\n        {{#if product.can_purchase}}\r\n          <button type=\"submit\" class=\"button button-primary button-wide add-to-cart button-progress spinner\" data-button-purchase>\r\n            <span class=\"button-text\">\r\n              {{#if product.pre_order}}\r\n                {{lang 'product.pre_order'}}\r\n              {{else}}\r\n                {{lang 'product.add_to_cart'}}\r\n              {{/if}}\r\n            </span>\r\n          </button>\r\n        {{else}}\r\n          {{#if product.out_of_stock}}\r\n            <div class=\"button button-primary button-wide add-to-cart button-disabled\">\r\n              {{lang 'product.sold_out'}}\r\n            </div>\r\n          {{/if}}\r\n        {{/if}}\r\n      </form>\r\n\r\n      <div class=\"product-actions\">\r\n        {{#if settings.show_wishlist}}\r\n          <form method=\"post\" class=\"wishlist-form\" action=\"{{product.add_to_wishlist_url}}\" {{#if customer}}data-customer{{/if}}>\r\n            <button type=\"submit\" class=\"button button-secondary button-wide add-to-wishlist button-progress spinner\">\r\n              <span class=\"button-text\">{{lang 'product.add_to_wishlist'}}</span>\r\n            </button>\r\n          </form>\r\n        {{/if}}\r\n\r\n        <div class=\"product-message-area\" data-product-message></div>\r\n\r\n        {{#if quick_view}}\r\n          <a class=\"quick-shop-details-lower link\" href=\"{{this.product.url}}\">{{lang 'product.view_full_details'}}</a>\r\n        {{/if}}\r\n      </div>\r\n\r\n      {{#any settings.add_this.buttons service='facebook_like'}}\r\n        <div class=\"share-block\">\r\n          <span class=\"form-label-text share-title\">{{lang 'product.share'}}</span>\r\n          {{> components/common/share-buttons share_buttons=settings.add_this url=product.url image=(getImage product.main_image 'thumb') title=product.title}}\r\n        </div>\r\n      {{/any}}\r\n\r\n      {{{snippet 'product_details'}}}\r\n    </div>\r\n  </div>\r\n</section>\r\n","components/products/product-single-images":"<div class=\"product-images-container\">\n  <div class=\"product-slides-container\">\n    <div class=\"product-slides-wrap\">\n      {{#each images}}\n        <div class=\"product-image\" style=\"background-image:url({{getImage this 'zoom'}});\" href=\"{{getImage this 'zoom'}}\" data-product-image>\n          <img src=\"{{getImage this 'product'}}\" alt=\"{{alt}}\">\n        </div>\n      {{/each}}\n    </div>\n  </div>\n  <ul class=\"product-images-pagination {{#if images.length '==' 1}}hidden{{/if}}\">\n    {{#each images}}\n      <li class=\"pagination-item\">\n        <a href=\"#\" data-slide-to=\"{{@index}}\" style=\"background-image: url({{getImage this 'thumb'}});\">\n          <img class=\"sr-only\" src=\"{{getImage this 'thumb'}}\" alt=\"{{this.alt}}\">\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n</div>\n","components/products/ratings":"{{#for 1 5}}\n  <svg class=\"icon icon-star {{#if ../this.rating '>=' $index}}full{{/if}}\"><use xlink:href=\