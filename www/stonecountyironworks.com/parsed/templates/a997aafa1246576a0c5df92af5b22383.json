{"components/products/options/set-select":"\n<div class=\"form-field {{#if required }}required{{/if}}\" data-product-attribute=\"set-select\">\n  <label class=\"form-label\">\n    <div class=\"form-label-text product-option-title\">\n      {{display_name}}\n    </div>\n    <div class=\"form-select-wrapper\">\n      <select name=\"attribute[{{id}}]\" class=\"form-input form-select\">\n        {{#each values}}\n          <option value=\"{{id}}\" {{#if selected}}selected{{/if}} data-product-attribute-value=\"{{id}}\">{{label}}</option>\n        {{/each}}\n      </select>\n    </div>\n  </label>\n</div>\n"}
