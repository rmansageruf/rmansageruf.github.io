{"components/products/options/date":"\n<div class=\"form-field  {{#if required}}required{{/if}}\" data-product-attribute=\"date\">\n  <label class=\"form-label\">\n\n    <div class=\"form-label-text product-option-title\">\n      {{display_name}}\n    </div>\n    <div class=\"date-select-group\">\n      <div class=\"form-select-wrapper month\">\n        <select name=\"attribute[{{id}}][month]\" class=\"form-input form-select\">\n          <option disabled>{{lang 'date.month'}}</option>\n          {{#enumerate 1 12}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.month this}}selected=\"selected\"{{/equals}}>{{getShortMonth this}}</option>\n          {{/enumerate}}\n        </select>\n      </div>\n\n      <div class=\"form-select-wrapper day\">\n        <select name=\"attribute[{{id}}][day]\" class=\"form-input form-select\">\n          <option disabled>{{lang 'date.day'}}</option>\n          {{#enumerate 1 31}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.day this}}selected=\"selected\"{{/equals}}>{{this}}</option>\n          {{/enumerate}}\n        </select>\n      </div>\n\n      <div class=\"form-select-wrapper year\">\n        <select name=\"attribute[{{id}}][year]\" class=\"form-input form-select\">\n          <option disabled>{{lang 'date.year'}}</option>\n          {{#enumerate earliest_year latest_year}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.year this}}selected=\"selected\"{{/equals}}>{{this}}</option>\n          {{/enumerate}}\n        </select>\n      </div>\n    </div>\n\n  </label>\n</div>\n"}
