{"components/faceted-search/facets/hierarchy":"<ul class=\"facet-list-items {{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\" data-count=\"{{items_count}}\" data-facet-filter-wrapper>\n  {{#each items}}\n    <li class=\"facet-item\" data-facet-item>\n      <a href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n        {{title}}\n        {{#if ../show_product_counts}}<span class=\"facet-item-count\" data-facet-item-count>({{count}})</span>{{/if}}\n      </a>\n    </li>\n  {{/each}}\n</ul>\n"}
