{"components/faceted-search/facets/multi":"<ul class=\"facet-list-items {{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}} {{#if items_count '<' total_items}}has-additional-filters{{/if}}\" data-count=\"{{items_count}}\" data-facet-filter-wrapper>\n  {{#each items}}\n    <li class=\"facet-item {{#if @index '>=' 5}}hidden{{/if}} {{#if selected}}selected-facet{{/if}}\" data-facet-item>\n      <a href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n        {{title}}\n        {{#if ../show_product_counts}}<span class=\"facet-item-count\" data-facet-item-count>({{count}})</span>{{/if}}\n      </a>\n    </li>\n  {{/each}}\n\n  {{#if total_items \">\" 5}}\n    <li>\n      <a class=\"facet-toggle-more\" href=\"#\" data-facet-more>\n        {{lang 'faceted_search.show_more'}}\n        <svg class=\"icon icon-arrow-down\"><use xlink:href=\"#icon-arrow-down\" /></svg>\n      </a>\n    </li>\n  {{/if}}\n</ul>\n"}
