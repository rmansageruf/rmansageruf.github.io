{"components/products/options/set-radio":"\n<div class=\"form-field {{#if required }}required{{/if}}\" data-product-attribute=\"set-radio\">\n  <div class=\"product-option-title\">\n    {{display_name}}\n  </div>\n  <div class=\"form-inline\">\n    {{#each values}}\n      <label class=\"form-label radio\" data-product-attribute-value=\"{{id}}\">\n        <input type=\"radio\" class=\"form-input\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" {{#if selected}}checked{{/if}}>\n        <span class=\"form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n"}
