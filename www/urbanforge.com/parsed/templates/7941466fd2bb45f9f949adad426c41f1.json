{"components/emthemes-modez/category/ajax-products-by-category":"\r\n{{#each categories}}\r\n    {{#if id '==' ../category_id}}\r\n        <section class=\"emthemesModez-productsByCategory {{#if ../../style}}{{#if ../../../style '!=' ' '}}emthemesModez-productsByCategory--{{../../../../style}}{{/if}}{{/if}}\">\r\n            <div class=\"emthemesModez-productsByCategory-imageColumn\">\r\n                <figure class=\"emthemesModez-productsByCategory-figure\">\r\n                    <img class=\"emthemesModez-productsByCategory-image\" src=\"{{../../image}}\" alt=\"{{../../heading}}\">\r\n                    <figcaption class=\"emthemesModez-productsByCategory-figcaption\">\r\n                        <div class=\"emthemesModez-productsByCategory-figcaption-body\">\r\n                            <h3 class=\"emthemesModez-productsByCategory-heading\">{{../../heading}}</h3>\r\n                            <div class=\"emthemesModez-productsByCategory-text\">{{../../text}}</div>\r\n                            <a class=\"emthemesModez-productsByCategory-button button\" href=\"{{url}}\">{{../../button}}</a>\r\n                        </div>\r\n                    </figcaption>\r\n                </figure>\r\n            </div>\r\n\r\n            <div class=\"emthemesModez-productsByCategory-productsColumn\" data-emthemesmodez-products-by-category=\"{{url}}\">\r\n                {{> components/emthemes-modez/common/product-loading theme_settings=../../theme_settings}}\r\n            </div>\r\n        </section>\r\n    {{/if}}\r\n{{/each}}\r\n\r\n","components/emthemes-modez/common/product-loading":"<div class=\"emthemesModez-productLoading emthemesModez-productLoading--maxCol2\">\n\t<div class=\"emthemesModez-productLoading-item\"><img src=\"{{cdn theme_settings.image_product_loading}}\" alt=\"loading\" title=\"loading\" /></div>\n\t<div class=\"emthemesModez-productLoading-item\"><img src=\"{{cdn theme_settings.image_product_loading}}\" alt=\"loading\" title=\"loading\" /></div>\n\t<div class=\"emthemesModez-productLoading-item\"><img src=\"{{cdn theme_settings.image_product_loading}}\" alt=\"loading\" title=\"loading\" /></div>\n\t<div class=\"emthemesModez-productLoading-item\"><img src=\"{{cdn theme_settings.image_product_loading}}\" alt=\"loading\" title=\"loading\" /></div>\n</div>\n"}
